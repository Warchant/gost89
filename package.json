{
  "name": "gost89",
  "version": "0.2.0",
  "description": "Gost89 in TS",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "scripts": {
    "test": "jest",
    "audit": "npm run audit",
    "build": "npm run compile",
    "test:cov": "jest --coverage",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "prepare": "npm run compile",
    "pretest": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dstucrypt/gost89.git"
  },
  "keywords": [
    "gost89",
    "typescript"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "test"
    ],
    "rootDir": ".",
    "transformIgnorePatterns": [
      "node_modules/.*",
      ".*\\.js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true,
    "testRegex": ".spec.(j|t)s$",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "author": "Ilya Petrov",
  "contributors": [
    "Bohdan Vanieiev <warchantua@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dstucrypt/gost89/issues"
  },
  "homepage": "https://github.com/dstucrypt/gost89",
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "gts": "^2.0.2",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "buffer": ">= 3.0"
  }
}
